        <!-- アコーディオン -->
        <script>
    function toggleAccordion() {
        const accSingleTriggers = document.querySelectorAll('.js-acc-single-trigger');
        accSingleTriggers.forEach(trigger => trigger.addEventListener('click', toggleAccordionHelper));
    }
    
    function toggleAccordionHelper() {

        const thisItem = this.parentNode;
        const content = thisItem.querySelector('.accordion-single-content');
        const title = thisItem.querySelector('.accordion-single-title');
        const isOpen = thisItem.classList.contains('is-open');

        // アコーディオンの状態をトグル
        thisItem.classList.toggle('is-open');

        // ボタンのテキストを切り替える
        if (!isOpen) {
            title.textContent = '閉じる';  // アコーディオンが開いているときの共通のテキスト
        } else {
            title.textContent = title.getAttribute('data-close-text');  // アコーディオンが閉じているときのテキスト（各アコーディオンに固有）
        }

        // タイトルの位置を移動
        if (!isOpen) {
            thisItem.appendChild(title); // タイトルを一番下に移動
        }
    }

    toggleAccordion();
</script>
		
        <!-- アコーディオン -->
        <script type="text/javascript">
            window._mfq = window._mfq || [];
            (function () {
                var mf = document.createElement("script");
                mf.type = "text/javascript"; mf.defer = true;
                mf.src = "//cdn.mouseflow.com/projects/fff6f8ac-3713-4d0d-b80f-d8c7b3d794ad.js";
                document.getElementsByTagName("head")[0].appendChild(mf);
            })();
        </script>
		
		<!--  ▼アコーディオン  -->
		<script>
			document.addEventListener('DOMContentLoaded', function () {
			const toggleTitles = document.querySelectorAll('.toggle_title');

			toggleTitles.forEach(function (toggleTitle) {
				toggleTitle.addEventListener('click', function () {
					toggleTitle.classList.toggle('selected');
					const nextElement = toggleTitle.nextElementSibling;

					if (nextElement) {
						if (getComputedStyle(nextElement).display === 'none') {
							nextElement.style.display = 'block';
						} else {
							nextElement.style.display = 'none';
						}
					}
				});
			});
		});
		</script>

        <!-- スムーズスクロール -->
        <script type="text/javascript">
            $(function () {
                $('a[href^="#"]').click(function () {
                    var adjust = -80;
                    var speed = 400;
                    var href = $(this).attr("href");
                    var target = $(href == "#" || href == "" ? 'html' : href);
                    var position = target.offset().top + adjust;
                    $('body,html').animate({ scrollTop: position }, speed, 'swing');
                    return false;
                });
            });
        </script>

        <!-- 検索実装 -->
        <script type="text/javascript">
            $(function () {
                $.indexHTMLEventInit();
            });
        </script>

        <!-- データ収集コード -->
        <script type="text/javascript">
            (function (c, l, a, r, i, t, y) {
                c[a] = c[a] || function () { (c[a].q = c[a].q || []).push(arguments) };
                t = l.createElement(r); t.async = 1; t.src = "https://www.clarity.ms/tag/" + i;
                y = l.getElementsByTagName(r)[0]; y.parentNode.insertBefore(t, y);
            })(window, document, "clarity", "script", "gpv8n2cl85");
        </script>

<script>
// スライダーの高さを揃える
const sliderItems = document.querySelectorAll('.slider_item');
const noClinicSliderItems = document.querySelectorAll('.slider_no_clinic');
if (sliderItems) {
    const sliderItemsHeight = sliderItems[0].offsetHeight;
    noClinicSliderItems.forEach(item => {
        item.style.height = `${sliderItemsHeight}px`;
    });
}
</script>

<script>
// モーダル要素を作成する関数
function createModalElements() {
  // モーダルのコンテナを作成
  const modalContainer = document.createElement('div');
  modalContainer.id = 'imageModal';
  modalContainer.className = 'image-modal';
  
  // モーダルの内容を作成
  const modalContent = document.createElement('div');
  modalContent.className = 'modal-content';
  
  // 閉じるボタンを作成
  const closeButton = document.createElement('span');
  closeButton.className = 'close-button';
  closeButton.innerHTML = '&times;';
  closeButton.onclick = closeModal;
  
  // モーダル画像要素を作成
  const modalImage = document.createElement('img');
  modalImage.id = 'modalImage';
  
  // 要素を組み立て
  modalContent.appendChild(closeButton);
  modalContent.appendChild(modalImage);
  modalContainer.appendChild(modalContent);
  
  // ページにモーダルを追加
  document.body.appendChild(modalContainer);
  
  // モーダル外クリックで閉じる
  modalContainer.onclick = function(event) {
    if (event.target === modalContainer) {
      closeModal();
    }
  };
}

// モーダルを開く関数
function openModal(imgSrc) {
  const modal = document.getElementById('imageModal');
  const modalImg = document.getElementById('modalImage');
  
  // 画像を先に読み込んでおく
  modalImg.src = imgSrc;
  
  // まずモーダルを表示状態にする
  modal.style.display = 'flex';
  
  // 少し遅延させてからアニメーションクラスを追加
  setTimeout(function() {
    modal.classList.add('show');
  }, 10); // わずかな遅延でCSSトランジションが効くようにする
  
  // スクロール禁止
  document.body.style.overflow = 'hidden';
}

// モーダルを閉じる関数
function closeModal() {
  const modal = document.getElementById('imageModal');
  
  // アニメーションクラスを削除
  modal.classList.remove('show');
  
  // アニメーション完了後にモーダルを非表示にする
  setTimeout(function() {
    modal.style.display = 'none';
  }, 300); // トランジションの時間と同じ300ms
  
  // スクロール許可
  document.body.style.overflow = 'auto';
}

// クリックイベントの設定
function setupImageClickEvents() {
  // plan_scrollクラス内の全ての画像を取得
  const planScrollDivs = document.querySelectorAll('.plan_scroll');
  
  planScrollDivs.forEach(div => {
    // div内の全ての画像を取得
    const images = div.querySelectorAll('img');
    
    images.forEach(img => {
      // recom_poabクラス内の画像をチェック
      const isInRecomPoab = img.closest('.recom_poab') !== null;
      
      if (!isInRecomPoab) {
        // recom_poabクラス内ではない画像にのみクリックイベントを追加
        img.style.cursor = 'pointer';
        img.addEventListener('click', function() {
          openModal(this.src);
        });
      }
    });
  });
}

// DOMが読み込まれた後に実行
document.addEventListener('DOMContentLoaded', function() {
  createModalElements();
  setupImageClickEvents();
});

// ESCキーでモーダルを閉じる
document.addEventListener('keydown', function(event) {
  if (event.key === 'Escape') {
    closeModal();
  }
});
</script>

<script>
$(document).ready(function(){
    $('.tips-card-slider').slick({
        dots: true,             // ドットナビゲーション表示
        infinite: true,         // 無限ループ
        speed: 500,             // アニメーション速度
        slidesToShow: 1,        // 一度に表示するカード数
        slidesToScroll: 1,      // 一度にスクロールするカード数
        autoplay: false,        // カードなので自動再生はオフ推奨 (trueにしてもOK)
        autoplaySpeed: 5000,
        arrows: true,           // 左右矢印表示
        prevArrow: '<button type="button" class="slick-prev"></button>',
        nextArrow: '<button type="button" class="slick-next"></button>',
        pauseOnHover: true,
        adaptiveHeight: true,   // カードの高さが異なる場合に対応
        centerMode: true,       // 現在のスライドを中央に表示 (見栄えが良い場合が多い)
        centerPadding: '15%',   // centerMode時の左右のカードの見切れ具合 (画面幅に応じて調整)
        // variableWidth: true, // カード幅が異なる場合はtrue

        responsive: [
            {
                breakpoint: 768, // 768px以下
                settings: {
                    slidesToShow: 1,
                    centerPadding: '10px', // モバイルでは見切れを少なくする
                    arrows: true // モバイルでも矢印表示
                }
            },
             {
                breakpoint: 480, // 480px以下
                settings: {
                    slidesToShow: 1,
                    centerPadding: '0px', // ほぼ見切れなし
                    arrows: true
                }
            }
        ]
    });

    // ウィンドウリサイズ時の再計算（adaptiveHeightやcenterMode使用時に推奨）
    var resizeTimer;
    $(window).on('resize', function() {
        clearTimeout(resizeTimer);
        resizeTimer = setTimeout(function() {
            $('.tips-card-slider').slick('setPosition');
        }, 250);
    });
});
</script>

<script>
(function() {
    function initDatsumoSwiper() {
        var datsumoSwiperElement = document.getElementById('datsumoSwiper');
        if (!datsumoSwiperElement) return;
        
        var datsumoSwiper = new Swiper('#datsumoSwiper', {
            wrapperClass: 'datsumo-swiper-wrapper',
            slideClass: 'datsumo-swiper-slide',
            loop: true,
            speed: 500,
            effect: 'slide',
            grabCursor: true,
            centeredSlides: false, // 中央寄せを無効化
            slidesPerView: 1, // 常に1枚だけ表示
            spaceBetween: 30,
            autoHeight: true, // コンテンツの高さに合わせて自動調整
            navigation: {
                nextEl: '.datsumo-swiper-button-next',
                prevEl: '.datsumo-swiper-button-prev',
            },
            pagination: {
                el: '.datsumo-swiper-pagination',
                clickable: true,
                bulletClass: 'datsumo-swiper-pagination-bullet',
                bulletActiveClass: 'datsumo-swiper-pagination-bullet-active',
            },
            breakpoints: {
                768: {
                    slidesPerView: 1,
                    spaceBetween: 30,
                },
                480: {
                    slidesPerView: 1,
                    spaceBetween: 10
                }
            },
            on: {
                init: function() {
                    console.log('Datsumo Swiper initialized');
                    // 初期化時にドットの位置更新（カード下端から10px下に配置）
                    setTimeout(updatePaginationPosition, 100);
                }
            }
        });

        // 各カードの下端から10px下にページネーションを配置し、コンテナの高さも調整する関数
        function updatePaginationPosition() {
            var container = document.querySelector('.datsumo-tips-container');
            var swiperContainer = document.querySelector('.datsumo-swiper-container');
            var activeSlide = document.querySelector('.swiper-slide-active');
            if (!activeSlide || !container || !swiperContainer) return;
            
            var activeCard = activeSlide.querySelector('.datsumo-tip-card');
            if (!activeCard) return;
            
            var containerRect = swiperContainer.getBoundingClientRect();
            var cardRect = activeCard.getBoundingClientRect();
            
            // カードの下端から10px下の位置を算出
            var newTop = (cardRect.bottom - containerRect.top) + 10;
            
            var pagination = document.querySelector('.datsumo-swiper-pagination');
            if (pagination) {
                pagination.style.bottom = 'auto';
                pagination.style.top = newTop + 'px';
            }
            
            // コンテナの高さをカード+ページネーション+余白に合わせて調整
            var paginationRect = pagination ? pagination.getBoundingClientRect() : { height: 20 };
            var newContainerHeight = (cardRect.height + paginationRect.height + 30) + 'px';
            swiperContainer.style.height = newContainerHeight;
            container.style.height = (cardRect.height + paginationRect.height + 50) + 'px';
        }

        // スライド切替時にドット位置を更新
        datsumoSwiper.on('slideChange', function() {
            setTimeout(updatePaginationPosition, 300);
        });
        
        // スライド初期化後にドット位置を更新
        datsumoSwiper.on('afterInit', function() {
            setTimeout(updatePaginationPosition, 300);
        });
        
        // ウィンドウリサイズ時にもドット位置を更新
        window.addEventListener('resize', function() {
            setTimeout(updatePaginationPosition, 300);
        });
        
        // 初期化直後と少し遅れてもう一度位置更新（確実に位置を合わせるため）
        setTimeout(updatePaginationPosition, 100);
        setTimeout(updatePaginationPosition, 500);
        
        return function cleanup() {
            window.removeEventListener('resize', updatePaginationPosition);
        };
    }
    
    if (document.readyState === 'loading') {
        document.addEventListener('DOMContentLoaded', initDatsumoSwiper);
    } else {
        initDatsumoSwiper();
    }
})();
</script>
<script type="module" src="https://datsumo-osusume-guide.com/wp-content/themes/article01/js/link_handler.js"></script>

<script>
//Tipsを01の時は非表示にするスクリプト
document.addEventListener('DOMContentLoaded', function() {
  const urlParams = new URLSearchParams(window.location.search);
  const keywordParam = urlParams.get('keyword');
  
  if (keywordParam && keywordParam.includes('01_datsumo')) {
    document.querySelectorAll('[content-none-01="tips"]').forEach(element => {
      element.style.display = 'none';
    });
    console.log('キーワードパラメータが 01_datsumo のため、content-none-01="tips" 属性を持つ要素を非表示にしました');
  }
});
</script>

