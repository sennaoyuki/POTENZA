    '02_datsumo_normal': '人気の脱毛はどこ？',
    '02_datsumo_normal_zenshin': '全身脱毛はどこ？',
    '02_datsumo_normal_vio': 'VIO脱毛はどこ？',
    '02_datsumo_normal_back': '背中脱毛はどこ？',
    '02_datsumo_normal_underarm': 'ワキ脱毛はどこ？',
    '02_datsumo_normal_arm': 'うで脱毛はどこ？',
    '02_datsumo_iryo': '人気の医療脱毛はどこ？',
    '02_datsumo_iryo_zenshin': '全身脱毛はどこ？',
    '02_datsumo_iryo_vio': 'VIO脱毛はどこ？',
    '02_datsumo_iryo_back': '背中脱毛はどこ？',
    '02_datsumo_iryo_underarm': 'ワキ脱毛はどこ？',
    '02_datsumo_iryo_arm': 'うで脱毛はどこ？'
  };

  const clinicCardAreaTitleBeforeMap = {
    '01_datsumo_iryo': '医療',
    '01_datsumo_normal': '女性',
    '01_datsumo_normal_zenshin': '全身',
    '01_datsumo_normal_vio': 'VIO',
    '01_datsumo_normal_back': '背中',
    '01_datsumo_normal_underarm': 'ワキ',
    '01_datsumo_normal_arm': 'うで',
    '02_datsumo_normal': '女性',
    '02_datsumo_normal_zenshin': '全身',
    '02_datsumo_normal_vio': 'VIO',
    '02_datsumo_normal_back': '背中',
    '02_datsumo_normal_underarm': 'ワキ',
    '02_datsumo_normal_arm': 'うで',
    '02_datsumo_iryo': '医療',
    '02_datsumo_iryo_zenshin': '全身',
    '02_datsumo_iryo_vio': 'VIO',
    '02_datsumo_iryo_back': '背中',
    '02_datsumo_iryo_underarm': 'ワキ',
    '02_datsumo_iryo_arm': 'うで'
  };

  targetElements.forEach(element => {
    if (element.classList.contains('sokyumvtit') && textMap[keyword]) {
      element.innerHTML = textMap[keyword];
    } else if (element.classList.contains('card-title') && cardTextMap[keyword]) {
      element.innerHTML = cardTextMap[keyword];
    } else if (element.classList.contains('table_content_title_after') && tableContentTitleMap[keyword]) {
      element.innerHTML = tableContentTitleMap[keyword];
    } else if (element.classList.contains('mv_left_copy_after') && mvLeftCopyAfterMap[keyword]) {
      element.innerHTML = mvLeftCopyAfterMap[keyword];
    } else if (element.classList.contains('clinic-card_area_title_before') && clinicCardAreaTitleBeforeMap[keyword]) {
      element.innerHTML = clinicCardAreaTitleBeforeMap[keyword];
    }
  });

  //02を含むとき比較表のタイトルを変更
  if (keyword && keyword.includes('02')) {
    const tableContentTitleBeforeElements = document.querySelectorAll('.table_content_title_before');
    tableContentTitleBeforeElements.forEach(element => {
      element.innerHTML = '今なら予約が取れる';
    });
  }

  //normalを含むとき比較表内の項目名を変更
  if (keyword && keyword.includes('normal')) {
    const rankingTableTd1Elements = document.querySelectorAll('.ranking-table_td1');
    rankingTableTd1Elements.forEach(element => {
      element.innerHTML = 'クリニック';
    });
  }
});
</script>
<script>
  const tabs = document.querySelectorAll(".tab");
  const contents = document.querySelectorAll(".tab-content");

  tabs.forEach(tab => {
    tab.addEventListener("click", () => {
      // タブの active 切り替え
      tabs.forEach(t => t.classList.remove("active"));
      tab.classList.add("active");

      // コンテンツの表示切り替え
      contents.forEach(c => c.classList.remove("active"));
      const target = document.getElementById(tab.dataset.tab);
      target.classList.add("active");
    });
  });
</script>
<script>
function checkNormalParameter() {
    const urlParams = new URLSearchParams(window.location.search);
    let hasNormal = false;
    
    for (const [key, value] of urlParams) {
        if (value.toLowerCase().includes('normal')) {
            hasNormal = true;
            break;
        }
    }
    
    const normalOnlyElement = document.querySelector('.normalOnly-js');
    if (normalOnlyElement) {
        normalOnlyElement.style.display = hasNormal ? 'block' : 'none';
    }
}
document.addEventListener('DOMContentLoaded', checkNormalParameter);
</script>
<script>
// URLパラメータに「eikyu」が含まれている場合のみ .eikyuOnly-js を表示
function checkEikyuParameter() {
    const urlParams = new URLSearchParams(window.location.search);
    let hasEikyu = false;
    
    // すべてのパラメータ値をチェック
    for (const [key, value] of urlParams) {
        if (value.toLowerCase().includes('eikyu')) {
            hasEikyu = true;
            break;
        }
    }
    
    // .eikyuOnly-js 要素を取得して表示/非表示を切り替え
    const eikyuOnlyElement = document.querySelector('.eikyuOnly-js');
    if (eikyuOnlyElement) {
        eikyuOnlyElement.style.display = hasEikyu ? 'block' : 'none';
    }
}

// ページ読み込み時に実行
document.addEventListener('DOMContentLoaded', checkEikyuParameter);

const bannerItems = document.querySelectorAll(".banner-item");
const clinicCards = document.querySelectorAll(".clinic-card");

bannerItems.forEach((banner, index) => {
    banner.addEventListener("click", () => {
        bannerItems.forEach(b => b.classList.remove("current"));
        banner.classList.add("current");

        clinicCards.forEach(card => card.classList.remove("current"));
        clinicCards[index].classList.add("current");
    });
});

const targetCaution = document.querySelector(".caution");
targetCaution.addEventListener("click", (e) => {
    const caution = e.target.nextElementSibling; 
    if(caution.classList.contains('caution-hidden')) {
        caution.classList.remove('caution-hidden');
    } else {
        caution.classList.add('caution-hidden');
    }
});
</script>

<script>
function checkVioParameter() {
    const urlParams = new URLSearchParams(window.location.search);
    const allParams = window.location.search;
    return allParams.includes('vio');
}

function switchBuiKwImages() {
    if (checkVioParameter()) {
        const buiKwImages = document.querySelectorAll('img.BuiKw');
        
        buiKwImages.forEach(function(img) {
            if (!img.hasAttribute('data-original')) {
                img.setAttribute('data-original', img.src);
            }
            
            img.src = 'https://datsumo-osusume-guide.com/wp-content/uploads/2025/06/eminal-main-vio-only.png';
            img.alt = 'VIO用スライダー画像';
        });
    } else {
        const buiKwImages = document.querySelectorAll('img.BuiKw');
        
        buiKwImages.forEach(function(img) {
            if (img.hasAttribute('data-original')) {
                img.src = img.getAttribute('data-original');
                img.alt = 'スライダー画像1';
            }
        });
    }
}

document.addEventListener('DOMContentLoaded', function() {
    switchBuiKwImages();
});

window.addEventListener('popstate', function() {
    switchBuiKwImages();
});
</script>

<script>
// URLパラメータに「keyword=02_datsumo_osusume」がある場合のみ .datsumo-osusume-only を表示
function checkDatsumoOsusumeKeyword() {
    const urlParams = new URLSearchParams(window.location.search);
    
    // keywordパラメータの値が「02_datsumo_osusume」かチェック
    const keywordValue = urlParams.get('keyword');
    const shouldShow = keywordValue === '02_datsumo_osusume';
    
    // .datsumo-osusume-only 要素を取得して表示/非表示を切り替え
    const targetElements = document.querySelectorAll('.datsumo-osusume-only');
    targetElements.forEach(element => {
        element.style.display = shouldShow ? 'block' : 'none';
    });
    
    console.log(shouldShow ? 'keyword=02_datsumo_osusumeが検出されました。コンテンツを表示します。' : 'keyword=02_datsumo_osusumeが見つかりません。コンテンツを非表示にします。');
}

// URLパラメータに「keyword=02_datsumo_osusume」がある場合のみ .datsumo-osusume-hidden を非表示
function hideDatsumoOsusumeKeyword() {
    const urlParams = new URLSearchParams(window.location.search);
    
    // keywordパラメータの値が「02_datsumo_osusume」かチェック
    const keywordValue = urlParams.get('keyword');
    const shouldHide = keywordValue === '02_datsumo_osusume';
    
    // .datsumo-osusume-hidden 要素を取得して表示/非表示を切り替え
    const targetElements = document.querySelectorAll('.datsumo-osusume-hidden');
    targetElements.forEach(element => {
        element.style.display = shouldHide ? 'none' : 'block';
    });
    
    console.log(shouldHide ? 'keyword=02_datsumo_osusumeが検出されました。コンテンツを非表示にします。' : 'keyword=02_datsumo_osusumeが見つかりません。コンテンツを表示します。');
}

// ページ読み込み時に実行
document.addEventListener('DOMContentLoaded', function() {
    checkDatsumoOsusumeKeyword();
    hideDatsumoOsusumeKeyword();
});
</script>
<script>
document.addEventListener('DOMContentLoaded', function() {
    const allCurrentCards = document.querySelectorAll('.clinic-card.current');
    if (allCurrentCards.length > 0) {
        // 最後のcurrent（output=25）を削除
        const lastCurrentCard = allCurrentCards[allCurrentCards.length - 1];
        lastCurrentCard.classList.remove('current');
    }
});
</script>

<!-- 下から表示されるポップアップメニュー -->
<script>
document.addEventListener('DOMContentLoaded', () => {
    const bmbs = document.querySelectorAll('.bottom_modal_btn');
    const bmcCloses = document.querySelectorAll(".close_bmc");
    const bmcClinics = document.querySelectorAll(".bmc_clinic");
    const bmc = document.querySelector(".bottom_modal_contents");
    
    bmbs.forEach((b) => {
        b.addEventListener("click", (e) => {
            const clinic = e.target.dataset.clinic;
            document.querySelector("html").classList.add('body_not_scroll');
            bmc.classList.remove("hidden");
            bmcClinics.forEach((clinic) => clinic.style.display = "none");
            document.querySelector(`.bmc_${clinic}`).style.display = "block";
        })
    });
    
    bmcCloses.forEach((b) => {
        b.addEventListener("click", () => {
            document.querySelector("html").classList.remove('body_not_scroll');
            bmc.classList.add("hidden");
        });
    });
});
</script>

<script>
// クリックトラッカー
const GAS_WEB_APP_URL = 'https://script.google.com/macros/s/AKfycbz13M4a7POL5bBvWnMcaLjdwNUpNy2unftxHeVDvYEJXdN_JovCPLWrDhRyIsOoCaX2/exec';

function initClickTracker() {
  console.log('Click tracker initialized');
  
  document.addEventListener('click', function(event) {
    const clickedElement = event.target;
    const elementInfo = getElementInfo(clickedElement);
    const scrollDepth = calculateScrollDepth(event);
    const timestamp = getJapanTimeStamp();
    const data = {
      elementType: elementInfo.type,
      elementId: elementInfo.id,
      elementClass: elementInfo.className,
      elementText: elementInfo.text,
      elementHref: elementInfo.href,
      elementTag: elementInfo.tag,
      scrollDepth: scrollDepth,
      browserWidth: window.innerWidth,
      browserHeight: window.innerHeight,
      clickX: event.pageX,
      clickY: event.pageY,
      url: window.location.href,
      timestamp: timestamp,
      title: document.title
    };

      console.log('クリックデータ:', data);

      sendDataToGAS(data);
  });
}

function getJapanTimeStamp() {
  const now = new Date();
  const japanTime = new Date(now.getTime() + (9 * 60 * 60 * 1000));
  const year = japanTime.getUTCFullYear();
  const month = String(japanTime.getUTCMonth() + 1).padStart(2, '0');
  const day = String(japanTime.getUTCDate()).padStart(2, '0');
  const hours = String(japanTime.getUTCHours()).padStart(2, '0');
  const minutes = String(japanTime.getUTCMinutes()).padStart(2, '0');
  const seconds = String(japanTime.getUTCSeconds()).padStart(2, '0');
  
  return `${year}-${month}-${day} ${hours}:${minutes}:${seconds}`;
}

function getElementInfo(element) {
  const result = {
    type: detectElementType(element),
    id: element.id || 'no-id',
    className: element.className || 'no-class',
    tag: element.tagName ? element.tagName.toLowerCase() : 'unknown',
    text: getElementText(element),
    href: (element.tagName === 'A' ? element.href : '') || 'no-href'
  };
  
  return result;
}

function getElementText(element) {
  if (element.tagName === 'IMG') {
    const altText = element.getAttribute('alt') || '';
    const srcPath = element.getAttribute('src') || '';
    const fileName = srcPath.split('/').pop();
    
    if (altText) {
      return `[IMG:${altText}]`;
    } else if (fileName) {
      return `[IMG:${fileName}]`;
    } else {
      return '[IMG:unnamed]';
    }
  }
  
  const text = (element.innerText || element.textContent || 'no-text').trim();
  return text.substring(0, 100);
}

function detectElementType(element) {
  const tagName = element.tagName ? element.tagName.toLowerCase() : '';
  const className = element.className || '';
  const classStr = className.toString();
  const role = element.getAttribute('role') || '';
  const ariaExpanded = element.getAttribute('aria-expanded');

if (tagName === 'a') {
    return 'link';
  }
  
  if (tagName === 'button' || role === 'button') {
    return 'button';
  }
  
  if (tagName === 'input' || tagName === 'textarea' || tagName === 'select') {
    return 'form-' + tagName;
  }
  
  if (
    (role === 'button' && ariaExpanded !== null) ||
    classStr.includes('accordion') ||
    (element.id && element.id.includes('accordion')) ||
    (element.dataset && element.dataset.toggle === 'collapse')
  ) {
    return 'accordion';
  }
  
  if (
    classStr.includes('slider') || 
    classStr.includes('carousel') ||
    classStr.includes('prev') || 
    classStr.includes('next') ||
    classStr.includes('arrow')
  ) {

    if (classStr.includes('prev') || classStr.includes('previous')) {
      return 'slider-prev';
    } else if (classStr.includes('next')) {
      return 'slider-next';
    } else {
      return 'slider';
    }
  }
  
  if (tagName === 'img') {
    return 'image';
  }
  
  if (
    tagName === 'nav' ||
    role === 'navigation' ||
    classStr.includes('menu') ||
    classStr.includes('nav')
  ) {
    return 'menu';
  }
  
  if (tagName === 'div' || tagName === 'span') {
    return tagName;
  }
  
  if (tagName === 'h1' || tagName === 'h2' || tagName === 'h3' || tagName === 'h4' || tagName === 'h5' || tagName === 'h6') {
    return 'heading';
  }
  
  if (tagName === 'p') {
    return 'paragraph';
  }
  
  return tagName || 'unknown';
}

function calculateScrollDepth(event) {

    const docHeight = Math.max(
    document.body.scrollHeight,
    document.body.offsetHeight,
    document.documentElement.clientHeight,
    document.documentElement.scrollHeight,
    document.documentElement.offsetHeight
  );
  
  const clickY = event.pageY;
  
  return ((clickY / docHeight) * 100).toFixed(1);
}

function sendDataToGAS(data) {
  const params = new URLSearchParams();
  for (const key in data) {
    params.append(key, data[key]);
  }
  
  const fetchOptions = {
    method: 'POST',
    mode: 'no-cors',
    body: params
  };
  
  fetch(GAS_WEB_APP_URL, fetchOptions)
    .then(() => console.log('データ送信成功'))
    .catch(error => console.error('データ送信エラー:', error));
}

window.addEventListener('load', initClickTracker);
// クリックトラッカーここまで
</script>
  </body>

  </html>