<script type="text/javascript" src="https://datsumo-osusume-guide.com/js/search/common.js?ver=1.0.0" id="mytheme-script-js"></script>
  <!-- ハンバーガーメニューとモーダルのスクリプト -->
        <script>
            window.onload = function () {
                var nav = document.getElementById('nav-wrapper');
                var hamburger = document.getElementById('js-hamburger');
                var blackBg = document.getElementById('js-black-bg');
                var links = document.querySelectorAll('.sp-nav a'); // メニュー内のすべてのリンク

                hamburger.addEventListener('click', function () {
                    nav.classList.toggle('open');
                });

                blackBg.addEventListener('click', function () {
                    nav.classList.remove('open');
                });

                // メニュー内のリンクがクリックされたとき
                links.forEach(function (link) {
                    link.addEventListener('click', function () {
                        nav.classList.remove('open');
                    });
                });

                // 履歴エントリの設定
                var currentUrl = window.location.href;
                history.replaceState({ browserBack: true }, null, currentUrl + '#');
                history.pushState(null, null, currentUrl);

                // ポップアップを表示
                window.addEventListener('popstate', function (event) {
                    if (event.state.browserBack) {
                        document.getElementById('choice').classList.add('active');
                    }
                });

                // モーダルを閉じるボタン
                var closeBtn = document.querySelector('.madal_contents span');
                if (closeBtn) {
                    closeBtn.addEventListener('click', function () {
                        document.getElementById('choice').classList.remove('active');
                    });
                }

                // ページが離脱される前にポップアップを表示
                window.addEventListener('beforeunload', function (event) {
                    document.getElementById('choice').classList.add('active');
                    // 離脱をキャンセルする場合は以下のコメントを解除します
                    // event.preventDefault();
                    // event.returnValue = '';
                });
            };
        </script>
		
		<!--スライーダコンテンツがない場合はタイトルも非表示にする処理-->
		<script>
		document.addEventListener("DOMContentLoaded", function () {
		  const sliderContents = document.querySelectorAll('.sc');
		  sliderContents.forEach((content) => {
			if (content && content.innerHTML.trim() === '') {
			  let previousP = content.previousElementSibling;
			  previousP.style.display = 'none';
			}
		  }); 
		});
		</script>

